    "express": "^4.18.2",
    "express-form-data": "^2.0.22"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-form-data": "^2.0.2",
    "@types/node": "^20.6.0",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
}
